#: venv/lib/python3.10/site-packages/django/contrib/flatpages/admin.py:13
msgid "Advanced options"
msgstr "Opções avançadas"

#: venv/lib/python3.10/site-packages/django/contrib/flatpages/apps.py:8
msgid "Flat Pages"
msgstr "Páginas Simples"

#: venv/lib/python3.10/site-packages/django/contrib/flatpages/forms.py:11
#: venv/lib/python3.10/site-packages/django/contrib/flatpages/models.py:9
msgid "URL"
msgstr "URL"

#: venv/lib/python3.10/site-packages/django/contrib/flatpages/forms.py:15
msgid ""
"Example: “/about/contact/”. Make sure to have leading and trailing slashes."
msgstr ""
"Exemplo: “/about/contact/”. Certifique-se que tem traços oblíquos no início "
"e no final."

#: venv/lib/python3.10/site-packages/django/contrib/flatpages/forms.py:20
msgid ""
"This value must contain only letters, numbers, dots, underscores, dashes, "
"slashes or tildes."
msgstr ""
"Este valor apenas poderá conter letras, números, pontos, underscores, "
"hífenes, barras ou tils."

#: venv/lib/python3.10/site-packages/django/contrib/flatpages/forms.py:34
msgid "Example: “/about/contact”. Make sure to have a leading slash."
msgstr ""
"Exemplo: “/about/contact”. Certifique-se que tem um traço obliquo no inicio."

#: venv/lib/python3.10/site-packages/django/contrib/flatpages/forms.py:47
msgid "URL is missing a leading slash."
msgstr "Está a faltar a barra inicial no URL."

#: venv/lib/python3.10/site-packages/django/contrib/flatpages/forms.py:52
msgid "URL is missing a trailing slash."
msgstr "Está a faltar a barra final no URL."

#: venv/lib/python3.10/site-packages/django/contrib/flatpages/forms.py:69
#, python-format
msgid "Flatpage with url %(url)s already exists for site %(site)s"
msgstr "A página plana com o url %(url)s já existe no site %(site)s"

#: venv/lib/python3.10/site-packages/django/contrib/flatpages/models.py:10
msgid "title"
msgstr "título"

#: venv/lib/python3.10/site-packages/django/contrib/flatpages/models.py:11
msgid "content"
msgstr "conteúdo"

#: venv/lib/python3.10/site-packages/django/contrib/flatpages/models.py:12
msgid "enable comments"
msgstr "permitir comentários"

#: venv/lib/python3.10/site-packages/django/contrib/flatpages/models.py:14
msgid "template name"
msgstr "nome da template"

#: venv/lib/python3.10/site-packages/django/contrib/flatpages/models.py:18
msgid ""
"Example: “flatpages/contact_page.html”. If this isn’t provided, the system "
"will use “flatpages/default.html”."
msgstr ""
"Exemplo: “flatpages/contact_page.html”. Se isto não for fornecido, o sistema "
"utilizará “flatpages/default.html”."

#: venv/lib/python3.10/site-packages/django/contrib/flatpages/models.py:23
msgid "registration required"
msgstr "é necessário registo"

#: venv/lib/python3.10/site-packages/django/contrib/flatpages/models.py:25
msgid "If this is checked, only logged-in users will be able to view the page."
msgstr ""
"Se estiver selecionado, apenas utilizadores autenticados poderão ver esta "
"página."

#: venv/lib/python3.10/site-packages/django/contrib/flatpages/models.py:29
msgid "sites"
msgstr "site"

#: venv/lib/python3.10/site-packages/django/contrib/flatpages/models.py:33
msgid "flat page"
msgstr "página plana"

#: venv/lib/python3.10/site-packages/django/contrib/flatpages/models.py:34
msgid "flat pages"
msgstr "páginas planas"
